{
  "name": "vitepress-starter",
  "description": "Graia 社区文档",
  "repository": "https://github.com/GraiaCommunity/Docs",
  "keywords": [
    "graia-ariadne",
    "graia-project",
    "graiax"
  ],
  "author": "GraiaCommunity",
  "license": "MIT",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "serve": "vitepress serve docs",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write docs/",
    "minfont": "node scripts/minfont.cjs"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/hash-sum": "^1.0.2",
    "@types/markdown-it": "^13.0.9",
    "@types/markdown-it-container": "^2.0.10",
    "@types/markdown-it-footnote": "^3.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.11.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "fflate": "^0.8.2",
    "hash-sum": "^2.0.0",
    "markdown-it": "^13.0.2",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.3",
    "mermaid": "^10.9.1",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "rollup-plugin-external-globals": "^0.9.2",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-font": "^1.3.0",
    "vitepress": "^1.3.1",
    "vue": "^3.4.34"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom",
        "rollup"
      ]
    }
  },
  "packageManager": "pnpm@8.15.6"
}
